{
	"commands": {
		"www": "./commands/www",
		"console": "./commands/repl"
	},
	"generators": {
		"options": {
			"engine": "ejs",
			"linker": true,
			"adapter": "sails-disk",
			"_maxHops": 100
		},
		"modules": {
			"new": "sails-generate-new",
			"backend": "sails-generate-backend",
			"frontend": "sails-generate-frontend",
			"gitigore": "sails-generate-gitigore",
			"packagejson": "sails-generate-packagejson",
			"readme": "sails-generate-readme",
			"model": "sails-generate-model",
			"controller": "sails-generate-controller",
			"api": "sails-generate-api",
			"policy": "sails-generate-policy",
			"response": "sails-generate-response",
			"view": "sails-generate-view",
			"adapter": "sails-generate-adapter",
			"generator": "sails-generate-generator",
			"hook": "sails-generate-hook"
		}
	},
	"hooks": {
		"moduleloader": "sails-hook-moduleloader",
		"request": "sails-hook-request",
		"orm": "sails-hook-orm",
		"views": "sails-hook-views",
		"blueprints": "sails-hook-blueprints",
		"responses": "sails-hook-responses",
		"controllers": "sails-hook-controllers",
		"sockets": "sails-hook-sockets",
		"pubsub": "sails-hook-pubsub",
		"policies": "sails-hook-policies",
		"services": "sails-hook-services",
		"csrf": "sails-hook-csrf",
		"cors": "sails-hook-cors",
		"i18n": "sails-hook-i18n",
		"userconfig": "sails-hook-userconfig",
		"session": "sails-hook-session",
		"grunt": "sails-hook-grunt",
		"http": "sails-hook-http",
		"userhooks": "sails-hook-userhooks",
		"logger": "sails-hook-logger"
	}
}